<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.coach.dao.AppVersionDao">
  
  <select id="getIdByVersion" parameterType="java.util.Map" resultType="Integer">
    select id from app_version where name = #{appVersion} and osType = #{osType}
  </select>
  
</mapper>